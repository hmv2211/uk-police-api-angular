{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "police-api-app": {
      "projectType": "application",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "options": {
            "outputPath": "dist/police-api-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/ployfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "serve": {
          "options": {
            "browserTarget": "police-api-app:build"
          }
        }
      }
    }
  },
  "defaultProject": "police-api-app"
}
